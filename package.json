  "license": "MIT",
  "dependencies": {
    "@openid/appauth": "^1.1.1",
    "@types/react": "^16.7.1",
    "@types/react-dom": "^16.0.9",
    "electron": "^3.0.8",
    "material-design-lite": "^1.3.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "typescript": "^3.1.6"
  }
}
